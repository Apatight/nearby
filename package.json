    "express": "^4.16.3",
    "faker": "^4.1.0",
    "gnomon": "^1.5.0",
    "jest": "^22.4.3",
    "jquery": "^3.3.1",
    "mongoose": "^5.0.15",
    "newrelic": "^4.1.0",
    "p": "^0.2.0",
    "path": "^0.12.7",
    "pg": "^7.4.1",
    "pg-promise": "^8.4.0",
    "react": "^16.3.1",
    "react-bootstrap": "^0.32.1",
