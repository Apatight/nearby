  "devDependencies": {
    "webpack": "^3.11.0",
    "webpack-cli": "^2.0.14"
  },
  "scripts": {
    "start": "node ./server/index.js",
    "start:watch": "nodemon ./server/index.js",
    "seed": "mongoimport --jsonArray -d apateez-nearby -c restaurants --file fullList.json --numInsertionWorkers 8",
    "build": "webpack -d --watch",
    "build:prod": "webpack -p",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker": "npm run build:prod && npm run seed && npm start",
    "generate": "node createDummyData.js",
    "fix": "truncate -s -1 fullList.json && echo ] >> fullList.json"
  },
