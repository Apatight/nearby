  "jest": {
    "globals": {
      "BASE_URL": "http://127.0.0.1:3000"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/client/src/components/TestSetup.js"
  },
  "homepage": "https://github.com/Apatight/nearby#readme"
